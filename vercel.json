{
  "version": 2,
  "builds": [
    {
      "src": "simplified_responsive_app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "simplified_responsive_app.py"
    }
  ],
  "env": {
    "PYTHONPATH": "/var/task"
  },
  "functions": {
    "simplified_responsive_app.py": {
      "runtime": "python3.9"
    }
  }
}