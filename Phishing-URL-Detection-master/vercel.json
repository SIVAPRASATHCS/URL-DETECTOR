{
  "version": 2,
  "name": "phishing-url-detector",
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "ENVIRONMENT": "production",
    "MAX_REQUESTS_PER_MINUTE": "100"
  },
  "functions": {
    "api/index.py": {
      "maxDuration": 30
    }
  }
}